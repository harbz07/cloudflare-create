# Example environment variables for local development
# Copy this file to `.env` and fill values (do NOT commit `.env` to git)

# Cloudflare / Wrangler
CF_ACCOUNT_ID=your-cloudflare-account-id
CF_API_TOKEN=your-cloudflare-api-token

# Optional AI Gateway (if using Cloudflare AI Gateway)
AI_GATEWAY_ID=your-ai-gateway-id

# Example external services
# (This project does not require Neo4j, but keeping an example for reference)
NEO4J_URI=neo4j+s://your-neo4j-host
NEO4J_USER=neo4j
NEO4J_PASSWORD=changeme

# Local dev flags
DEV=true
